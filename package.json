{
  "name": "hypract",
  "version": "0.0.6",
  "description": "Hyper productive components for Firebase built with preact, material-ui and redux",
  "license": "MIT",
  "main": "dist/hypract.js",
  "minified:main": "dist/hypract.min.js",
  "es2015": "lib-esm/index.js",
  "module": "lib/index.js",
  "typings": "dist/hypract.d.ts",
  "author": "Anthony Ferrando <ferrando.tony@gmail.com>",
  "homepage": "https://github.com/Mytrill/hypract",
  "bugs": {
    "url": "https://github.com/Mytrill/hypract/issues"
  },
  "scripts": {
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack && npm run build:dts",
    "build:dts": "dts-bundle --name hypract --main lib/index.d.ts --baseDir lib --out ../dist/hypract.d.ts",
    "clean": "shx rm -rf dist lib lib-esm",
    "example": "webpack "
  },
  "dependencies": {
    "@types/material-ui": "0.18.0",
    "@types/react": "15.6.2",
    "dot-prop-immutable": "1.3.1",
    "material-ui": "0.19.0",
    "preact": "8.2.1",
    "preact-compat": "3.17.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-form": "7.0.3",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.73",
    "awesome-typescript-loader": "^3.2.3",
    "dts-bundle": "0.7.3",
    "npm-run-all": "^4.0.2",
    "shx": "^0.2.2",
    "ts-loader": "2.3.2",
    "typescript": "2.4.2",
    "webpack": "3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Mytrill/hypract.git"
  },
  "files": [
    "src",
    "dist",
    "lib",
    "lib-esm"
  ]
}
